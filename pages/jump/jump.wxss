page{
  height: 100%;
  background-color: rgb(173,212,183);
}
.video_detail{
  width: 90%;
  height: 80%;
  position: absolute;
  top: 5%;
  left: 5%;
  justify-content: center;
/* border: 1rpx solid white */
}
.video_title{
  display: flex;
  justify-content: space-between;
  color: #067C02;
  font-size: 38rpx;
  margin-bottom: 10rpx;
  font-weight: bold
}
.video_vi{
  position: relative;
  border-color: grey;
  margin-bottom: 20rpx;
}
.video_vi video{
  width: 100%;
  border-radius: 30rpx;
}
